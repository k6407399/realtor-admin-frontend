# Base URL
REACT_APP_BACKEND_URL=http://localhost:5000/api/v1

# Admin Authentication and Blocklist Routes
REACT_APP_ADMIN_LOGIN=/admin/login
REACT_APP_ADMIN_BLOCKLIST=/admin/blocklist
REACT_APP_ADMIN_UNBLOCKLIST=/admin/users/:id/block

# Properties Form Fields
REACT_APP_FORM_FIELDS=/properties/:type/form-fields

# Land Routes
REACT_APP_LAND_CREATE=/properties/land
