# Flats Routes
REACT_APP_FLATS_CREATE=/properties/flats
REACT_APP_FLATS_FETCH=/properties/flats
REACT_APP_FLATS_UPDATE=/properties/flats/:id
REACT_APP_FLATS_DELETE=/properties/flats/:id

# Villas Routes
REACT_APP_VILLAS_CREATE=/properties/villas
REACT_APP_VILLAS_FETCH=/properties/villas
REACT_APP_VILLAS_UPDATE=/properties/villas/:id
REACT_APP_VILLAS_DELETE=/properties/villas/:id

# Apartments Routes
REACT_APP_APARTMENTS_CREATE=/properties/apartments
REACT_APP_APARTMENTS_FETCH=/properties/apartments
REACT_APP_APARTMENTS_UPDATE=/properties/apartments/:id
REACT_APP_APARTMENTS_DELETE=/properties/apartments/:id

# Approvals Routes
REACT_APP_APPROVALS_FETCH=/properties/approvals
